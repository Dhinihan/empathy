<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkDialog" id="spell_dialog">
    <property name="border_width">5</property>
    <property name="title" translatable="yes">Spell Checker</property>
    <property name="modal">True</property>
