msgstr "Доступен"

#: ../libempathy/empathy-utils.c:248
msgid "Busy"
msgstr "Занят"

#: ../libempathy/empathy-utils.c:251
msgid "Away"
msgstr "Отошёл"

#: ../libempathy/empathy-utils.c:253
msgid "Invisible"
msgstr "Невидимый"

#: ../libempathy/empathy-utils.c:255
msgid "Offline"
msgstr "Не в сети"

